{
  "version": 2,
  "builds": [
    {
      "src": "index.html",  
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(favicon.ico|images|static)/",   
      "headers": { "Cache-Control": "max-age=31536000, immutable" },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ]
}
